{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src"],
  "entryPointStrategy": "expand",
  "out": "docs",
  "name": "Angular Auto Import Extension",
  "readme": "README.md",
  "plugin": ["typedoc-plugin-markdown"],
  "exclude": [
    "**/node_modules/**",
    "**/out/**",
    "**/dist/**",
    "**/test/**",
    "**/*.test.ts",
    "**/*.spec.ts"
  ],
  "excludePrivate": false,
  "excludeProtected": false,
  "excludeInternal": false,
  "excludeNotDocumented": false,
  "includeVersion": true,
  "sourceLinkTemplate": "https://github.com/ngx-rock/vscode-angular-auto-import/blob/main/{path}#L{line}",
  "gitRevision": "main",
  "gitRemote": "origin",
  "disableSources": false,
  "categorizeByGroup": false,
  "validation": {
    "invalidLink": true,
    "notExported": true
  },
  "compilerOptions": {
    "skipLibCheck": true,
    "strict": true
  },
  "externalSymbolLinkMappings": {
    "@types/vscode": {
      "CompletionList": "#",
      "Selection": "#"
    }
  },
  "fileExtension": ".md",
  "hidePageHeader": false,
  "hideBreadcrumbs": false,
  "hidePageTitle": false,
  "mergeReadme": true,
  "indexFormat": "table",
  "parametersFormat": "table",
  "propertiesFormat": "table",
  "typeDeclarationFormat": "table",
  "outputFileStrategy": "modules",
  "textContentMappings": {
    "header.title": "Angular Auto Import Extension API Documentation",
    "header.docs": "Generated documentation for the Angular Auto Import VS Code extension"
  }
} 